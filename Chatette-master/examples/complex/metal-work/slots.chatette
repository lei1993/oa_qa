;----------------- Time ----------------------
@[filter_time]
  ~[late]
  ~[on time]

;---------------- Completion ------------------
@[filter_completion]
  ~[fully planned] = 1.0
  ~[planned at$1.0] = 1.0
  ~[planned at$0.9] = 0.9
  ~[planned at$0.8] = 0.8
  ~[planned at$0.7] = 0.7
  ~[planned at$0.6] = 0.6
  ~[planned at$0.5] = 0.5
  ~[planned at$0.4] = 0.4
  ~[planned at$0.3] = 0.3
  ~[planned at$0.2] = 0.2
  ~[planned at$0.1] = 0.1
  ~[planned at$0.0] = 0.0
  ~[not planned] = 0.0

;------------------ Utilization ------------------
@[utilization#positive]
  ~[fully used] = 1.0
  ~[used at$100] = 1.0
  ~[used at$90] = 0.9
  ~[used at$80] = 0.8
  ~[used at$70] = 0.7
  ~[used at$60] = 0.6
  ~[used at$50] = 0.5
  ~[used at$40] = 0.4
  ~[used at$30] = 0.3
  ~[used at$20] = 0.2
  ~[used at$10] = 0.1
  ~[used at$0] = 0.0
  ~[not used] = 0.0
@[utilization#negative]
  not ~[fully used] = not fully
@[utilization#number only]
  ~[fully used] = 1.0
  ~[utilization$100] = 1.0
  ~[utilization$90] = 0.9
  ~[utilization$80] = 0.8
  ~[utilization$70] = 0.7
  ~[utilization$60] = 0.6
  ~[utilization$50] = 0.5
  ~[utilization$40] = 0.4
  ~[utilization$30] = 0.3
  ~[utilization$20] = 0.2
  ~[utilization$10] = 0.1
