~[prodline$ACRONYM]
  ~[production line] $ACRONYM
  the ~[production line] ~[named] $ACRONYM
  the $ACRONYM ~[production line]

~[GLO_SSP#long]
    ~[prodline$GLO_SSP]
    ~[prodline$glo_ssp]
~[LAL_HSM#long]
    ~[prodline$LAL_HSM]
    ~[prodline$lal_hsm]
    {our/the [internal?]} ~[line name#hot strip mill] ~[production line?]
    the ~[line name#hot strip mill] ~[production line?] of ~[client factory name]
~[LAL_SKP#long]
